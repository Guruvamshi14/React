# Ignore node modules
node_modules/

# Ignore environment files
.env
.env.local
.env.*.local

# Ignore build directories
dist/
build/

# Ignore logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Ignore system files
.DS_Store
Thumbs.db

# Ignore editor folders
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# Ignore coverage reports
coverage/

# Ignore OS generated files
*.swp
*.bak
*.tmp

# Ignore lock files (optional, comment out if you want to keep them)
package-lock.json
yarn.lock
pnpm-lock.yaml

# Ignore other cache folders
.cache/
.next/
out/

# Ignore database files
*.sqlite
*.sqlite3
*.db

# Ignore temporary and archive files
*.tmp
*.temp
*.zip
